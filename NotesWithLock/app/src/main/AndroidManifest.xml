<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.noteswithlock" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/lock1"

        android:theme="@style/AppTheme" >
        <activity
            android:name=".WelcomeScreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginCheck"
            android:label="@string/app_name"
            android:icon="@drawable/login">
            <intent-filter>
                <action android:name="com.noteswithlock.Logincheck" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".OpenNotes"
            android:label="@string/no_name" >
            <intent-filter>
                <action android:name="com.noteswithlock.OpenNotes" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".NewText"
            android:label=" " >
        </activity>
    </application>

</manifest>
